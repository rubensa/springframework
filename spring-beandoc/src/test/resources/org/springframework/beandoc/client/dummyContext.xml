<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <description>
    	Bean factory definition for junit tests - defines a ContextProcessor that does nothing
    </description>
    
    <bean id="processor" class="org.springframework.beandoc.DummyContextProcessor">
    	<constructor-arg index="0"><value>${input.files}</value></constructor-arg>
     	<constructor-arg index="1"><value>${output.dir}</value></constructor-arg>
    </bean>  
    
</beans>
