<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>	
	<bean id="foo" class="com.foo.Bar" abstract="true"/>
		
	<bean id="foo2" parent="foo"/>
	
	<bean id="foo3" class="whatever">
		<property name="dep"><ref local="foo4"/></property>
		<property name="dep2"><ref bean="foo5"/></property>		
	</bean>
	
	<bean id="foo4"/>
	
	<bean id="foo99" class="whatever">
		<property name="target">
			<bean id="foo66" class="something"/>
		</property>		
	</bean>
	
	<bean id="rep1" class="something.else">
		<replaced-method name="foo" replacer="bar"/>
	</bean>
</beans>
