<?xml version="1.0" encoding="UTF-8"?>


<process-definition>
	
	<start-state>
		<transition to="myState">
		</transition>
	</start-state>
	
	<state name="myState">
		<transition to="end">
			<action name="myAction" config-type="bean" class="org.springframework.workflow.jbpm.SpringHandler">
				<beanName>jbpmAction</beanName>
			</action>
		</transition>
	</state>
	
	<end-state name="end"/>
	
</process-definition>