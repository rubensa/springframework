<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">

<!-- Application context definition -->

<beans>

	<bean id="flowRegistry" class="org.springframework.webflow.config.registry.XmlFlowRegistryFactoryBean">
		<property name="definitionLocations">
			<list>
				<value>classpath:org/springframework/webflow/config/registry/flow1.xml</value>
				<value>classpath:org/springframework/webflow/config/registry/flow2.xml</value>
			</list>
		</property>
	</bean>
	
	<bean id="flow1" class="org.springframework.webflow.config.registry.XmlFlowFactoryBean">
		<property name="location" value="classpath:org/springframework/webflow/config/registry/flow1.xml"/>
	</bean>

	<bean id="flow2" class="org.springframework.webflow.config.registry.XmlFlowFactoryBean">
		<property name="location" value="classpath:org/springframework/webflow/config/registry/flow2.xml"/>
	</bean>

</beans>