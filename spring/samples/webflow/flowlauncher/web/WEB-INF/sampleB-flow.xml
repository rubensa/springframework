<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE webflow PUBLIC "-//SPRING//DTD WEBFLOW//EN"
	"http://www.springframework.org/dtd/spring-webflow.dtd">

<webflow id="sampleB" start-state="captureInput">

	<action-state id="captureInput">
		<action class="org.springframework.samples.flowlauncher.web.flow.action.SampleAction"/>
		<transition on="success" to="bPage"/>
	</action-state>
	
	<view-state id="bPage" view="bPage">
		<transition on="end" to="end"/>
	</view-state>
	
	<end-state id="end"/>

</webflow>