<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans_2_0.dtd"
>

<beans>

	<bean 
		class="org.springframework.aop.aspectj.annotation.AnnotationAwareAspectJAutoProxyCreator" />

	<bean 
		class="org.springframework.aop.aspectj.annotation.AbstractAspectJAdvisorFactoryTests$TwoAdviceAspect" 
		singleton="true" />
		
	<bean id="adrian" class="org.springframework.beans.TestBean" singleton="false">
		<property name="name" value="adrian" />
		<property name="age" value="34" />
	</bean>
	
	
	
</beans>
